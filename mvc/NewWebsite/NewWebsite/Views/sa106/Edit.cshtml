			</div>
		</div>
	</div>
</div>

<!-- Script to Delete Record -->
<script type="text/javascript">
    function DeleteItem(id, name) {
        $("#deleteModal").modal('show');
        $("#deleteModal #YesDeleteBtn").attr("href", "/sa106/Delete/" + id);
        $("#deleteMessage").text("Are you sure you want to delete this sa106 - " + name + " ?");
    }
</script>

