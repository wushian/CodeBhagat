					<a id="btnDelete" class="btn btn-danger" onclick='DeleteItem("@Model.gkey","@Model.mr200gkey")'><span class="glyphicon glyphicon-remove"></span> Delete</a>
				</div>
				<div class="pull-right">
					<button type="submit" value="Save" class="btn btn-primary"><span class="glyphicon glyphicon-floppy-disk"></span> Save</button>
					&nbsp;
					<a href="@Url.Action("Index", "sa106")" class="btn btn-primary"><span class="glyphicon glyphicon-arrow-left"></span> Back to List</a>
				</div>
			</div>
		</div>
	</div>
        </fieldset>
		}
	}

